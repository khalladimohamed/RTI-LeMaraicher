.SILENT:

GPP = g++ -Wall 

OBJS = CreationBD.o LibSockets.o moc_windowclient.o windowclient.o
PROGRAMS = mainclient

mainclient:	mainclient.cpp CreationBD.o LibSockets.o moc_windowclient.o windowclient.o
	$(GPP) -I -o main mainclient.cpp CreationBD.o LibSockets.o moc_windowclient.o windowclient.o

CreationBD.o:		CreationBD.cpp 
	$(GPP) -I -o CreationBD.o CreationBD.cpp -c

LibSockets.o:		LibSockets.cpp 
	$(GPP) -I -o LibSockets.o LibSockets.cpp -c

moc_windowclient.o:	moc_windowclient.cpp 
	$(GPP) -I -o moc_windowclient.o moc_windowclient.cpp -c

windowclient.o:		windowclient.cpp 
	$(GPP) -I -o windowclient.o windowclient.cpp -c


clean:	
	rm -f $(OBJS) core
clobber:	clean
	rm -f $(PROGRAMS)
